// routes/uiRoutes.js
const express = require('express');
const { getAllUiItems, addUiItem } = require('../controllers/uiController');
const { protect } = require('../middleware/authMiddleware');

const router = express.Router();

router.get('/', getAllUiItems);
router.post('/', protect, addUiItem); // âœ… Protected

module.exports = router;
